<!DOCTYPE html>
<html>
  <head>
    <title><%= full_title(yield(:title)) %></title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag "application_admin", media: "all", "data-turbolinks-track": "reload" %>
    <%= javascript_pack_tag "application", media: "all", "data-turbolinks-track": "reload" %>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <link rel="icon" href="images/fevicon.png" type="image/gif"/>
  </head>

  <body>
    <div id="wrapper">
      <%= render "admin/layouts/left" %>
      <%= render "admin/layouts/right" %>
    </div>
    <%= render "admin/layouts/footer" %>
  </body>
</html>
