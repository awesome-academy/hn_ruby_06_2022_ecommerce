<tr>
  <td><%= order_counter + 1 %></td>
  <td><%= order.id %></td>
  <td><%= l order.created_at, format: :long %></td>
  <td><%= l order.updated_at, format: :long %></td>
  <td><%= number_to_currency(order.total_money) %></td>
  <%= status_order order.status.to_sym %>
  <td>
    <%= link_to order_path(order.id), remote: true do %>
        <label class="btn btn-warning">
        <i class="fa fa-info-circle fs-3" aria-hidden="true"></i>
      </label>
    <% end %>
  </td>
</tr>
